<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd">
<mapper namespace="mapper.DevUserMapper">
	
	<!-- 根据用户名查询 -->
	<select id="getByCode" resultType="DevUser" >
		select * from dev_user where devCode=#{devCode}
	</select>
	
</mapper>